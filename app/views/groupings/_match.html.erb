<tr data-match-id='<%= match.id %>'>
  <td>
    <button type="button" class="btn btn-default btn-xs remove-saved-match">
      <span class="glyphicon glyphicon-trash"></span>
    </button>
  </td>
  <td><%= match.search_token %></td>
  <td><%= match.contribution.candidate_name %></td>
  <td><%= match.contribution.office %></td>
  <td><%= "(ID #{match.contribution.id}) #{match.contribution.contributor_name}" %></td>
  <td><%= match.contribution.formatted_full_address %></td>
  <td><%= "#{match.contribution.amount} (Agg: #{match.contribution.aggregate})" %></td>
  <td><%= match.contribution.election_period %></td>
</tr>
